{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@autoinquirer/formly": {
      "projectType": "library",
      "root": "projects/autoinquirer/formly",
      "sourceRoot": "projects/autoinquirer/formly/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/autoinquirer/formly/tsconfig.lib.json",
            "project": "projects/autoinquirer/formly/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/autoinquirer/formly/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/autoinquirer/formly/src/test.ts",
            "tsConfig": "projects/autoinquirer/formly/tsconfig.spec.json",
            "karmaConfig": "projects/autoinquirer/formly/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/autoinquirer/formly/tsconfig.lib.json",
              "projects/autoinquirer/formly/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "demo",
      "sourceRoot": "demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo",
            "index": "demo/src/index.html",
            "main": "demo/src/main.ts",
            "polyfills": "demo/src/polyfills.ts",
            "tsConfig": "demo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "demo/src/favicon.ico",
              "demo/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/ace-builds/src-min",
                "output": "./assets/ace-builds/"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/font-awesome/css/font-awesome.min.css",
              "node_modules/ngx-markdown-editor/assets/highlight.js/agate.min.css",
              "demo/src/styles.scss"
            ],
            "scripts": [
              "node_modules/ngx-markdown-editor/assets/highlight.js/highlight.min.js",
              "node_modules/ngx-markdown-editor/assets/marked.min.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "demo/src/environments/environment.ts",
                  "with": "demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        }
      }
    },
    "@autoinquirer/markdown": {
      "projectType": "library",
      "root": "projects/autoinquirer/markdown",
      "sourceRoot": "projects/autoinquirer/markdown/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/autoinquirer/markdown/tsconfig.lib.json",
            "project": "projects/autoinquirer/markdown/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/autoinquirer/markdown/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/autoinquirer/markdown/src/test.ts",
            "tsConfig": "projects/autoinquirer/markdown/tsconfig.spec.json",
            "karmaConfig": "projects/autoinquirer/markdown/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/autoinquirer/markdown/tsconfig.lib.json",
              "projects/autoinquirer/markdown/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "@autoinquirer/auth": {
      "projectType": "library",
      "root": "projects/autoinquirer/auth",
      "sourceRoot": "projects/autoinquirer/auth/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/autoinquirer/auth/tsconfig.lib.json",
            "project": "projects/autoinquirer/auth/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/autoinquirer/auth/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/autoinquirer/auth/src/test.ts",
            "tsConfig": "projects/autoinquirer/auth/tsconfig.spec.json",
            "karmaConfig": "projects/autoinquirer/auth/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/autoinquirer/auth/tsconfig.lib.json",
              "projects/autoinquirer/auth/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "@autoinquirer/formly"
}